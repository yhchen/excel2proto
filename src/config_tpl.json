{
    "IncludeFilesAndPath": [
        "./testcase"
    ],
    "ColorToGroupMap": {
        "FFFF00": "YELLOW",
        "FF0000": "RED",
        "00B050": "GREEN",
        "000000": "WHITE",
        "FFFFFF": "BLACK",
        "*": "ALL"
    },
    "Export": [
        {
            "type": "proto3",
            "OutputDir": "./test/exports/global_proto3.proto",
            "OutputDataDir": "./test/exports/proto3-data/",
            "ExtName": ".bytes",
            "GroupFilter": [
                "ALL"
            ],
            "ExportTemple": "{data}",
            "Namespace": "Hotfix",
            "UseDefaultValueIfEmpty": false,
            "NameTranslate": {
                "Id": "_id"
            }
        }
    ],
    "see: http://momentjs.com/docs/#/parsing/string-format/": null,
    "DateFmt": "YYYY/MM/DD HH:mm:ss",
    "TypeCheckerJSFilePath": "./testcase/type_extens_checker.js",
    "cm21": "relative to ${cwd} or absolute path",
    "TinyDateFmt": "YYYY/MM/DD",
    "TimeStampUseMS": true,
    "CustomDataNode": "B1",
    "cm22": "custom defined data, can be null",
    "LineBreak": "\n",
    "FractionDigitsFMT": 6,
    "EnableTypeCheck": true,
    "EnableDebugOutput": true
}